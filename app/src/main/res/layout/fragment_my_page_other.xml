<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.landing.myPage.tab.more.MyPageOtherFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/txtTitle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@color/textViewBackground"
            android:fontFamily="@font/noto_sans_regular"
            android:includeFontPadding="false"
            android:paddingStart="28dp"
            android:paddingTop="12dp"
            android:paddingBottom="13dp"
            android:text="@string/other_page"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="RtlSymmetry" />

        <TextView
            android:id="@+id/txtForFirstTime"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:fontFamily="@font/noto_sans_regular"
            android:includeFontPadding="false"
            android:paddingStart="2dp"
            android:paddingTop="24dp"
            android:paddingBottom="21dp"
            android:text="@string/for_first_time_users"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/imgForFirstTimeTab"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtTitle"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/imgForFirstTimeTab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="23dp"
            android:contentDescription="@string/image_description"
            android:paddingTop="24dp"
            android:paddingEnd="8dp"
            android:paddingBottom="21dp"
            android:src="@drawable/ic_forward"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtTitle"
            tools:ignore="RtlSymmetry" />

        <View
            android:id="@+id/vwForFirstTime"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="18dp"
            android:background="?android:attr/listDivider"
            android:contentDescription="@string/image_description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtForFirstTime" />

        <TextView
            android:id="@+id/txtAboutApp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:fontFamily="@font/noto_sans_regular"
            android:includeFontPadding="false"
            android:paddingStart="2dp"
            android:paddingTop="18dp"
            android:paddingBottom="18dp"
            android:text="@string/about_this_app"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/imgAboutAppTab"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vwForFirstTime"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/imgAboutAppTab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="23dp"
            android:contentDescription="@string/image_description"
            android:paddingTop="18dp"
            android:paddingEnd="8dp"
            android:paddingBottom="18dp"
            android:src="@drawable/ic_forward"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/vwForFirstTime"
            tools:ignore="RtlSymmetry" />

        <View
            android:id="@+id/vwAboutApp"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="18dp"
            android:background="?android:attr/listDivider"
            android:contentDescription="@string/image_description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtAboutApp" />

        <TextView
            android:id="@+id/txtFAQ"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:fontFamily="@font/noto_sans_regular"
            android:includeFontPadding="false"
            android:paddingStart="2dp"
            android:paddingTop="21dp"
            android:paddingBottom="21dp"
            android:text="@string/faq_Inquiries"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/imgFAQTab"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vwAboutApp"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/imgFAQTab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="23dp"
            android:contentDescription="@string/image_description"
            android:paddingTop="21dp"
            android:paddingEnd="8dp"
            android:paddingBottom="21dp"
            android:src="@drawable/ic_forward"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/vwAboutApp"
            tools:ignore="RtlSymmetry" />

        <View
            android:id="@+id/vwFAQ"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="18dp"
            android:background="?android:attr/listDivider"
            android:contentDescription="@string/image_description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtFAQ" />

        <TextView
            android:id="@+id/txtOpinion"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:fontFamily="@font/noto_sans_regular"
            android:includeFontPadding="false"
            android:paddingStart="2dp"
            android:paddingTop="21dp"
            android:paddingBottom="21dp"
            android:text="@string/opinions_and_requests"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/imgOpinionTab"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vwFAQ"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/imgOpinionTab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="23dp"
            android:contentDescription="@string/image_description"
            android:paddingTop="21dp"
            android:paddingEnd="8dp"
            android:paddingBottom="21dp"
            android:src="@drawable/ic_forward"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/vwFAQ"
            tools:ignore="RtlSymmetry" />

        <View
            android:id="@+id/vwOpinion"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="18dp"
            android:background="?android:attr/listDivider"
            android:contentDescription="@string/image_description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtOpinion" />

        <TextView
            android:id="@+id/txtBrassoWebSite"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:fontFamily="@font/noto_sans_regular"
            android:includeFontPadding="false"
            android:paddingStart="2dp"
            android:paddingTop="21dp"
            android:paddingBottom="27dp"
            android:text="@string/brasso_official_website"
            android:textColor="@color/black"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/imgBrassoWebSiteTab"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vwOpinion"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/imgBrassoWebSiteTab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="23dp"
            android:contentDescription="@string/image_description"
            android:paddingTop="21dp"
            android:paddingEnd="8dp"
            android:paddingBottom="27dp"
            android:src="@drawable/ic_forward"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vwOpinion"
            tools:ignore="RtlSymmetry" />

        <View
            android:id="@+id/vwBrassoWebSite"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="18dp"
            android:background="?android:attr/listDivider"
            android:contentDescription="@string/image_description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtBrassoWebSite" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>